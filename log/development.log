  [1m[35mSQL (0.9ms)[0m  [1m[32mINSERT INTO "articles" ("headline", "textonly", "date_published", "portal_source_id", "created_at", "updated_at", "img", "origurl") VALUES (?, ?, ?, ?, ?, ?, ?, ?)[0m  [["headline", "В сквере на ул. Раззакова АЗС и кафе расположены близко к жилым домам"], ["textonly", "Народную новость прислал читательУважаемый Албек Ибраимович!Мы, жители домов ул. Раззакова, 2 и ул. Орозбекова, 1, неоднократно письменно обращались к руководителям города Бишкек, отдельным муниципальным органам, руководству УМС мэрии города Бишкек по вопросу обеспечения безопасности граждан и сохранения сквера на пересечении улиц Раззакова и Туголбай-Ата. На фото видно близкое (взрывоопасное) расположение АЗС и кафе \"Лесок\" (жарят шашлыки) к нашим домам. Кроме того, в павильоне \"Транзит\", расположенном в сквере, продаются спиртные напитки на разлив практически перед воротами дошкольного учреждения. Просим вас взять под личный контроль и разобраться с этой ситуацией!"], ["date_published", "2017-06-08"], ["portal_source_id", 2], ["created_at", "2017-06-10 17:21:30.405031"], ["updated_at", "2017-06-10 17:21:30.405031"], ["img", "http://data.vb.kg/image/small/2017-06-08_16-39-05_158673.jpg"], ["origurl", "http://www.vb.kg/doc/361255_v_skvere_na_yl._razzakova_azs_i_kafe_raspolojeny_blizko_k_jilym_domam.html"]]
