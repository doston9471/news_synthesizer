  [1m[35mSQL (0.4ms)[0m  [1m[32mINSERT INTO "articles" ("headline", "textonly", "date_published", "portal_source_id", "created_at", "updated_at", "origurl") VALUES (?, ?, ?, ?, ?, ?, ?)[0m  [["headline", "Президент Алмазбек Атамбаев вылетел с рабочим визитом в Казахстан"], ["textonly", "Сегодня, 8 июня, глава государства вылетел с рабочим визитом в город Астану (Казахстан). Об этом сообщает отдел информационной политики аппарата президента КР.В рамках визита Алмазбек Атамбаев примет участие в очередном заседании Совета глав государств-членов Шанхайской организации сотрудничества.Кроме того, вместе с главами государств, участвующими в заседании Совета ШОС, президент посетит торжественную церемонию открытия выставки \"ЭКСПО-2017\"."], ["date_published", "2017-06-08"], ["portal_source_id", 2], ["created_at", "2017-06-10 14:53:36.461420"], ["updated_at", "2017-06-10 14:53:36.461420"], ["origurl", "http://www.vb.kg/doc/361261_prezident_almazbek_atambaev_vyletel_s_rabochim_vizitom_v_kazahstan.html"]]
