  [1m[35mSQL (0.4ms)[0m  [1m[32mINSERT INTO "articles" ("headline", "textonly", "date_published", "portal_source_id", "created_at", "updated_at", "img", "origurl") VALUES (?, ?, ?, ?, ?, ?, ?, ?)[0m  [["headline", "В Бишкеке при входе в суд милиционер ударил журналиста"], ["textonly", "Сегодня, 8 июня, в Первомайском городском суде сотрудник Первомайского РОВД ударил журналиста Наргизу Токоеву в грудь. Представитель СМИ пыталась зайти на процесс по делу арестованных сторонников экс-депутата парламента Садыра Жапарова. Однако в связи с тем, что в здании суда проходил процесс лидера парламентской фракции \"Ата Мекен\" Омурбека Текебаева, сотрудники милиции перекрыли вход. Когда журналист попыталась объяснить, что она идет на процесс по другому делу, сотрудник милиции ее ударил в грудь."], ["date_published", "2017-06-08"], ["portal_source_id", 2], ["created_at", "2017-06-10 17:21:38.866103"], ["updated_at", "2017-06-10 17:21:38.866103"], ["img", "http://data.vb.kg/image/small/2017-06-08_16-02-43_152730.jpg"], ["origurl", "http://www.vb.kg/doc/361246_v_bishkeke_pri_vhode_v_syd_milicioner_ydaril_jyrnalista.html"]]
