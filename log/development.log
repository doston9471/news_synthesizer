  [1m[35mSQL (0.6ms)[0m  [1m[32mINSERT INTO "articles" ("headline", "textonly", "date_published", "portal_source_id", "created_at", "updated_at", "origurl") VALUES (?, ?, ?, ?, ?, ?, ?)[0m  [["headline", "Дуйшонкула Чотонова оставили под стражей в СИЗО ГКНБ"], ["textonly", "Сегодня, 8 июня, в Первомайском районном суде Бишкека продолжилось судебное разбирательство по делу лидера парламентской фракции \"Ата Мекен\" Омурбека Текебаева и бывшего министра чрезвычайных ситуаций, а также экс-посла КР в Корее Дуйшонкула Чотонова.Напомним, адвокат Чотонова Бактыбек Жумашев заявил ходатайство об изменении меры пресечения своему подзащитному, не связанную с арестом.Выйдя из совещательной комнаты судья Эрнис уулу Айбек вынес постановление о содержании Чотонова под стражей. Адвокаты намерены обжаловать данное решение в вышестоящей инстанции."], ["date_published", "2017-06-08"], ["portal_source_id", 2], ["created_at", "2017-06-10 16:13:36.528061"], ["updated_at", "2017-06-10 16:13:36.528061"], ["origurl", "http://www.vb.kg/doc/361211_dyyshonkyla_chotonova_ostavili_pod_strajey_v_sizo_gknb.html"]]
